# Application
ENVIRONMENT=development
SERVER_PORT=8080
LOG_LEVEL=info

# Database
# For Supabase, use DATABASE_URL (recommended)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Or use individual PostgreSQL parameters (not recommended for Supabase)
# DB_MODE=postgres
# DB_HOST=localhost
# DB_PORT=5432
# DB_USER=postgres
# DB_PASSWORD=postgres
# DB_NAME=apihub
# DB_SSLMODE=disable

# Use memory mode for demo without database
# DB_MODE=memory

# JWT
JWT_SECRET=your-jwt-secret-key-change-this

# CORS
FRONTEND_URL=http://localhost:5173
